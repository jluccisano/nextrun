					input.form-control(type="text" size="8" class="form-control" ng-model="race.timing.startingTime" data-autoclose="1" placeholder="{{ 'Heure' | translate }}" bs-timepicker)
			div.form-group
				label.col-lg-4.control-label(translate) Briefing
				div.form-group
					input.form-control(type="text" size="10" class="" ng-model="race.timing.briefingTime" data-autoclose="1" placeholder="{{ 'Date' | translate }}" bs-datepicker)
