					input.form-control(type="text" size="8" class="form-control" ng-model="race.timing.rewardsTime" data-autoclose="1" placeholder="{{ 'Heure' | translate }}" bs-timepicker)
			div.form-group
				label.col-lg-4.control-label(translate) Plus d'informations
			div.col-lg-12(text-angular text-angular-name="demo1" ng-model="race.timing.timingMoreInformation" ta-toolbar-class="btn-toolbar" ta-toolbar-group-class="btn-group" ta-toolbar-button-class="btn btn-default btn-xs" ta-toolbar-button-active-class="active")