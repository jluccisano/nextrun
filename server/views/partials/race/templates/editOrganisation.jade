div.container
	div.bs-callout.bs-callout-info
		h4 Infomations
		p Tous les champs  sont facultatifs. Tous les champs qui ne seront pas renseign√© n'apparaitront pas dans la vue finale.
	tabset(vertical="false" type="navType")
		tab(heading="race.timing")
			div.row
				div.col-lg-8
					form.form-horizontal(name="form1" role="form" style="margin-top:10px")
						div.form-group
							label.col-lg-4.control-label race.startingTime
							div.col-lg-4
								div(class="dropdown")
									a(class="dropdown-toggle my-toggle-select" id="dLabel" role="button" data-toggle="dropdown" data-target="#" href="#")
										div(class="input-group")
											input.form-control(name="startingTime" type="text" data-ng-model="race.timing.startingTime" data-date-time-input="HH:mm" placeholder="race.startingTime")
											span.input-group-addon
												i.glyphicon.glyphicon-time
									ul(class="dropdown-menu" role="menu" aria-labelledby="dLabel")
										datetimepicker(data-ng-model="race.timing.startingTime"  data-datetimepicker-config="{ dropdownSelector: '.my-toggle-select', startView:'hour' }")
						div.form-group
							label.col-lg-4.control-label race.briefingTime
							div.col-lg-4
								div(class="dropdown")
									a(class="dropdown-toggle my-toggle-select" id="dLabel" role="button" data-toggle="dropdown" data-target="#" href="#")
										div(class="input-group")
											input.form-control(name="briefingTime" type="text" data-ng-model="race.timing.briefingTime" data-date-time-input="HH:mm" placeholder="race.briefingTime") 
											span.input-group-addon
												i.glyphicon.glyphicon-time
									ul(class="dropdown-menu" role="menu" aria-labelledby="dLabel")
										datetimepicker(data-ng-model="race.timing.briefingTime"  data-datetimepicker-config="{ dropdownSelector: '.my-toggle-select', startView:'hour' }")
						div.form-group
							label.col-lg-4.control-label race.checkoutBibTime
							div.col-lg-4
								div(class="dropdown")
									a(class="dropdown-toggle my-toggle-select" id="dLabel" role="button" data-toggle="dropdown" data-target="#" href="#")
										div(class="input-group")
											input.form-control(name="checkoutBibTime" type="text" data-ng-model="race.timing.checkoutBibTime" data-date-time-input="HH:mm" placeholder="race.checkoutBibTime")
											span.input-group-addon
												i.glyphicon.glyphicon-time
									ul(class="dropdown-menu" role="menu" aria-labelledby="dLabel")
										datetimepicker(data-ng-model="race.timing.checkoutBibTime"  data-datetimepicker-config="{ dropdownSelector: '.my-toggle-select', startView:'hour' }")
						div.form-group
							label.col-lg-4.control-label race.rewardsTime
							div.col-lg-4
								div(class="dropdown")
									a(class="dropdown-toggle my-toggle-select" id="dLabel" role="button" data-toggle="dropdown" data-target="#" href="#")
										div(class="input-group")
											input.form-control(name="rewardsTime" type="text" data-ng-model="race.timing.rewardsTime" data-date-time-input="HH:mm" placeholder="race.rewardsTime")
											span.input-group-addon
												i.glyphicon.glyphicon-time
									ul(class="dropdown-menu" role="menu" aria-labelledby="dLabel")
										datetimepicker(data-ng-model="race.timing.rewardsTime"  data-datetimepicker-config="{ dropdownSelector: '.my-toggle-select', startView:'hour' }")
						div.form-group
							label.col-lg-4.control-label race.moreInformation
						div.col-lg-12(text-angular text-angular-name="demo1" data-ng-model="race.timing.timingMoreInformation" ta-toolbar-class="btn-toolbar" ta-toolbar-group-class="btn-group" ta-toolbar-button-class="btn btn-default btn-xs" ta-toolbar-button-active-class="active")
		tab(heading="race.pricing")
			div.row
				div.col-lg-8
					form.form-horizontal(name="form2" role="form" style="margin-top:10px")
						div.form-group
							label.col-lg-4.control-label race.memberFFPrice
							div.col-lg-2(class="input-group")
								input.form-control(name="memberFFPrice" type="number" data-ng-model="race.pricing.memberFFPrice")
								span.input-group-addon
									i.fa.fa-eur
						div.form-group
							label.col-lg-4.control-label race.notMemberFFPrice
							div.col-lg-2(class="input-group")
								input.form-control(name="notMemberFFPrice" type="number" data-ng-model="race.pricing.notMemberFFPrice")
								span.input-group-addon
									i.fa.fa-eur
						div.form-group
							label.col-lg-4.control-label race.moreInformation
						div.col-lg-12(text-angular text-angular-name="demo1" data-ng-model="race.pricing.pricingMoreInformation" ta-toolbar-class="btn-toolbar" ta-toolbar-group-class="btn-group" ta-toolbar-button-class="btn btn-default btn-xs" ta-toolbar-button-active-class="active")
		tab(heading="race.registration")
			div.row
				div.col-lg-12
					form.form-horizontal(name="form3" role="form" style="margin-top:10px")
						div.form-group
							label.col-lg-4.control-label race.registrationOpeningDate
							div.col-lg-3
								div(class="dropdown")
									a(class="dropdown-toggle my-toggle-select" id="dLabel" role="button" data-toggle="dropdown" data-target="#" href="#")
										div(class="input-group")
											input.form-control(type="text" data-ng-model="race.registration.registrationOpeningDate" data-date-time-input="DD-MM-YYYY HH:MM" placeholder="race.registrationOpeningDate")
											span.input-group-addon
												i.glyphicon.glyphicon-time
									ul(class="dropdown-menu" role="menu" aria-labelledby="dLabel")
										datetimepicker(data-ng-model="race.registration.registrationOpeningDate" data-datetimepicker-config="{ dropdownSelector: '.my-toggle-select', startView:'month', minView:'minute' }")
						div.form-group
							label.col-lg-4.control-label race.registrationLimitDate
							div.col-lg-3
								div(class="dropdown")
									a(class="dropdown-toggle my-toggle-select" id="dLabel" role="button" data-toggle="dropdown" data-target="#" href="#")
										div(class="input-group")
											input.form-control(type="text" data-ng-model="race.registration.registrationLimitDate" data-date-time-input="DD-MM-YYYY HH:MM" placeholder="race.registrationLimitDate")
											span.input-group-addon
												i.glyphicon.glyphicon-time
									ul(class="dropdown-menu" role="menu" aria-labelledby="dLabel")
										datetimepicker(data-ng-model="race.registration.registrationLimitDate"  data-datetimepicker-config="{ dropdownSelector: '.my-toggle-select', startView:'month', minView:'minute' }")
						div.form-group
							label.col-lg-4.control-label race.registrationWebSite
							div.col-lg-6
								input.form-control(name="registrationWebSite" data-ng-model="race.registration.registrationWebSite" placeholder="race.registrationWebSite")
						div.form-group
							label.col-lg-4.control-label race.registrationAddress
							div.col-lg-6
								textarea.form-control(name="registrationAddress" rows="5" data-ng-model="race.registration.registrationAddress" placeholder="race.registrationAddress")
						div.form-group
							label.col-lg-4.control-label race.registrationOnPlace
							div.col-lg-6
								label.radio-inline
									input(type="radio",name="radioGroup", data-ng-model="race.registration.registrationOnPlace", value="oui")
									span common.yes
								label.radio-inline
									input(type="radio",name="radioGroup", data-ng-model="race.registration.registrationOnPlace", value="non")
									span common.no
						div.form-group
							label.col-lg-4.control-label race.numberOfPlacesAvailable
							div.col-lg-2(class="input-group")
								input.form-control(name="numberOfPlacesAvailable" type="number" data-ng-model="race.registration.numberOfPlacesAvailable")
								span.input-group-addon race.places
						div.form-group
							label.col-lg-4.control-label race.moreInformation
						div.col-lg-8(text-angular text-angular-name="demo1" data-ng-model="race.registration.registrationMoreInformation" ta-toolbar-class="btn-toolbar" ta-toolbar-group-class="btn-group" ta-toolbar-button-class="btn btn-default btn-xs" ta-toolbar-button-active-class="active")
