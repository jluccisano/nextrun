div(class="container feedback")
	div(class="row")
		div(class="col-lg-4 col-lg-offset-4")
			div(class="panel panel-default")
				div(class="panel-heading") <strong>{{ 'Envoyer un message' | translate }}</strong>
				div(class="panel-body")
					include ./templates/feedback
				div(class="panel-footer")
					button(class="btn btn-primary" type="button" ng-click="submit(feedback)" ng-disabled="feedbackForm.$invalid" translate) Envoyer