							div.col-lg-8
								input.form-control(name="organizerWebSite" data-ng-model="race.organizerWebSite" placeholder=t("race.organizerWebSite"))
						div.form-group
							label.col-lg-4.control-label=t("race.moreInformation")
						div.col-lg-12(text-angular text-angular-name="demo1" data-ng-model="race.miscellaneous.miscellaneousMoreInformation" ta-toolbar-class="btn-toolbar" ta-toolbar-group-class="btn-group" ta-toolbar-button-class="btn btn-default btn-xs" ta-toolbar-button-active-class="active")