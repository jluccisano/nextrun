							div.col-lg-8
								input.form-control(name="address1" data-ng-model="race.plan.address.address1" placeholder=t("race.address"))
						div.form-group
							label.col-lg-4.control-label
							div.col-lg-8
								input.form-control(name="address2" data-ng-model="race.plan.address.address2" placeholder=t("race.moreAddress"))
						div.form-group
							label.col-lg-4.control-label=t("race.postcode")
							div.col-lg-8
